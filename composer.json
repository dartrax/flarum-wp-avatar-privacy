{
	"name": "dartrax/flarum-wp-avatar-privacy",
	"description": "Flarum Wordpress Avatar Privacy: Use the cached avatars from Avatar Privacy for Flarum",
	"keywords": [
		"flarum",
		"wordpress",
		"user",
		"avatar",
		"privacy"
	],
	"homepage": "https://github.com/dartrax/flarum-gravatar",
	"support": {
		"source": "https://github.com/AlexanderOMara/flarum-gravatar",
		"issues": "https://github.com/AlexanderOMara/flarum-gravatar/issues"
	},
	"license": "MPL-2.0",
	"type": "flarum-extension",
	"require": {
		"flarum/core": "^0.1.0-beta.15"
	},
	"autoload": {
		"psr-4": {
			"Dartrax\\FlarumWpAvatarPrivacy\\": "src/"
		}
	},
	"extra": {
		"flarum-extension": {
			"title": "WP Avatar Privacy",
			"icon": {
				"image": "icon.png",
				"backgroundColor": ""
			}
		}
	},
	"archive": {
		"exclude": [
			".*",
			"/scripts",
			"/docker",
			"/js",
			"!/js/dist"
		]
	}
}
